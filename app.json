{
  "expo": {
    "name": "Job Path",
    "slug": "Job-Path",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/growth.png",
    "scheme": "job-path",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/growth.png",
        "backgroundColor": "#FAFAFC"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.ishu62847.Job_Path"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/growth.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/growth.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {}
    }
  }
}
