{
  "expo": {
    "name": "job-search",
    "slug": "job-search",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/navigation.webp",
    "scheme": "jobsearch",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/navigation.webp",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.ishu62847.jobsearch"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/navigation.webp"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/navigation.webp",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "f3813746-4aa8-4d66-82bf-c1833bcdcff3"
      }
    }
  }
}
